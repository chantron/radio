<div id="user_list">
	<ul id="user_list">
		<% @users.each do | user | %>
		<li class="single_user">
			<h4 class="delta"><%= user.username %></h4>
			<div class="user_info">
				<p><%= user.first_name %> <%= user.last_name %></p>
				<p><a href="/admin/user/edit/<%= user.id %>">Edit</a></p>
			</div>
		</li>

		<% end %>
	</ul>
</div>